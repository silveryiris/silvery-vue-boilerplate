{
  "name": "silvery-vue-boilerplate",
  "version": "0.0.1",
  "description": "vue spa boilerplate",
  "repository": {
    "type": "git",
    "url": "git@github.com:silveryiris/silvery-vue-boilerplate.git"
  },
  "scripts": {
    "test": "mocha -r esm",
    "start:dev": "node -r esm server/server.js",
    "clean": "rimraf ./dist/**",
    "compile": "npx webpack -r esm --config build/webpack.config.js",
    "build": "npm run clean&&cross-env NODE_ENV=production npm run compile",
    "build:dev": "npm run clean&&cross-env NODE_ENV=development npm run compile"
  },
