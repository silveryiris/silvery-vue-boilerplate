    "rimraf": "^2.6.3",
    "stylus": "^0.54.5",
    "vue": "^2.6.10",
    "vue-meta": "^2.0.4",
    "vue-router": "^3.0.7",
    "vuex": "^3.1.1"
  },
  "devDependencies": {
